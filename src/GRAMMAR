PROGRAMP -> PROGRAM
PROGRAM -> STMT_LIST
STMT_LIST -> STMT STMT_LIST
STMT_LIST -> ''
STMT -> PRINTSTMT
STMT -> DOUBLEASMT
STMT -> INTASMT
STMT -> STRASMT
STMT -> EXPR END
EXPR -> I_EXPR
EXPR -> D_EXPR
EXPR -> S_EXPR
EXPR -> ID
PRINTSTMT -> PRINT L_PAREN EXPR R_PAREN END
DOUBLEASMT -> D_ASMT ID ASSIGN D_EXPR END
INTASMT -> I_ASMT ID ASSIGN I_EXPR END
STRASMT -> S_ASMT ID ASSIGN S_EXPR END
OP -> PLUS
OP -> MINUS
OP -> MULTI
OP -> DIVIDE
OP -> POWER
SIGN -> PLUS
SIGN -> MINUS
SIGN -> ''
D_EXPR -> ID
D_EXPR -> SIGN DOUBLE
D_EXPR -> SIGN DOUBLE OP SIGN DOUBLE
D_EXPR -> SIGN DOUBLE OP D_EXPR
D_EXPR -> D_EXPR OP  SIGN D_EXPR
D_EXPR -> D_EXPR OP D_EXPR
I_EXPR -> ID
I_EXPR -> SIGN INT
I_EXPR -> SIGN INT OP SIGN INT
I_EXPR -> SIGN INT OP I_EXPR
I_EXPR -> I_EXPR OP  SIGN I_EXPR
I_EXPR -> I_EXPR OP I_EXPR
S_EXPR -> ID
S_EXPR -> STRING
S_EXPR -> CONCAT L_PAREN S_EXPR COMMA S_EXPR R_PAREN
S_EXPR -> CHARAT L_PAREN S_EXPR COMMA I_EXPR R_PAREN