STMT -> EXPR end.
STMT -> REASMT.
REASMT -> id assign EXPR end.
EXPR -> ID_EXPR.
EXPR -> D_EXPR.
EXPR -> I_EXPR.
OP -> plus.
SIGN -> plus.
SIGN -> .
ID_EXPR -> id ID_EXPR2
ID_EXPR2 ->OP id ID_EXPR2
ID_EXPR2 ->''
D_EXPR -> SIGN double D_EXPR2
D_EXPR -> id OP SIGN double
D_EXPR -> SIGN double OP id
D_EXPR2 -> OP D_EXPR
D_EXPR2 -> ''
I_EXPR -> SIGN int I_EXPR2
I_EXPR -> id OP SIGN int
I_EXPR -> SIGN int OP id
I_EXPR2 -> OP I_EXPR
I_EXPR2 -> ''