EXPRP -> EXPR
EXPR -> SIGN DOUBLE D_EXPR
EXPR -> SIGN INT I_EXPR
EXPR ->  SIGN ID ID_EXPR
EXPR -> STRING S_EXPR
SIGN -> PLUS
SIGN -> ''
OP -> PLUS
RELOP -> LESS
ID_EXPR -> OP ID_EXPR2
ID_EXPR -> RELOP ID_EXPR3
ID_EXPR -> ''
ID_EXPR2 -> SIGN ID ID_EXPR
ID_EXPR2 -> SIGN INT I_EXPR
ID_EXPR2 -> SIGN DOUBLE D_EXPR
ID_EXPR3 -> SIGN ID I_EXPR
ID_EXPR3 -> SIGN INT I_EXPR
ID_EXPR3 -> SIGN DOUBLE I_EXPR
ID_EXPR3 -> SIGN STRING I_EXPR

D_EXPR -> OP D_EXPR2
D_EXPR -> RELOP D_EXPR3
D_EXPR -> ''
D_EXPR2 -> SIGN ID D_EXPR
D_EXPR2 -> SIGN DOUBLE D_EXPR
D_EXPR3 -> SIGN ID I_EXPR
D_EXPR3 -> SIGN DOUBLE I_EXPR

I_EXPR -> OP I_EXPR2
I_EXPR -> RELOP I_EXPR3
I_EXPR -> ''
I_EXPR2 -> SIGN ID D_EXPR
I_EXPR2 -> SIGN INT D_EXPR
I_EXPR3 -> SIGN ID I_EXPR
I_EXPR3 -> SIGN INT I_EXPR

S_EXPR -> RELOP D_EXPR3
S_EXPR -> ''
D_EXPR3 -> SIGN ID I_EXPR
D_EXPR3 -> SIGN STRING I_EXPR



