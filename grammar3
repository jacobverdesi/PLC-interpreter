EXPRP -> EXPR
EXPR -> D_EXPR
EXPR -> I_EXPR
EXPR ->  ID_EXPR
EXPR -> S_EXPR
SIGN -> PLUS
SIGN -> ''
OP -> PLUS
RELOP -> LESS

ID_EXPR -> SIGN ID ID_EXPR2
ID_EXPR2 -> OP ID_EXPR
ID_EXPR2 -> ''

D_EXPR -> SIGN DOUBLE D_EXPR2
D_EXPR -> SIGN ID OP D_EXPR
D_EXPR2 -> OP SIGN DOUBLE D_EXPR2
D_EXPR2 -> OP SIGN ID D_EXPR2
D_EXPR2 -> ''

I_EXPR -> SIGN INT I_EXPR2
I_EXPR -> SIGN ID OP I_EXPR
I_EXPR2 -> OP SIGN INT I_EXPR2
I_EXPR2 -> OP SIGN ID I_EXPR2
I_EXPR2 -> ''


I_EXPR -> B_EXPR
B_EXPR -> I_EXPR RELOP I_EXPR
B_EXPR -> ID_EXPR RELOP I_EXPR
B_EXPR -> I_EXPR RELOP ID_EXPR
B_EXPR -> D_EXPR RELOP D_EXPR
B_EXPR -> ID_EXPR RELOP D_EXPR
B_EXPR -> D_EXPR RELOP ID_EXPR
B_EXPR -> ID_EXPR RELOP ID_EXPR
B_EXPR -> ID_EXPR RELOP S_EXPR

B_EXPR -> S_EXPR RELOP S_EXPR
B_EXPR -> S_EXPR RELOP ID_EXPR


S_EXPR -> STRING
S_EXPR -> CONCAT L_PAREN S_EXPR COMMA S_EXPR R_PAREN
S_EXPR -> CONCAT L_PAREN ID COMMA S_EXPR R_PAREN
S_EXPR -> CONCAT L_PAREN S_EXPR COMMA ID R_PAREN
S_EXPR -> CHARAT L_PAREN S_EXPR COMMA I_EXPR R_PAREN